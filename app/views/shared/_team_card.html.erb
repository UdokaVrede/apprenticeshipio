<div class="team-member">
    <img class="team-member-profile-img" src=<%=locals[:img_url]%> alt="">
    <div class="team-member-description-bk">
    </div>
    <div class="team-member-name">
      <p><%=locals[:name]%></p>
    </div>
    <div  class="team-member-info">
      <p><%=locals[:job]%></p>
      <p><%=locals[:company]%></p>
      <p>in <%=locals[:location]%></p>
    </div>
    <div class="team-member-btn">
      <i class="fab fa-linkedin-in fa-lg"></i>
    </div>
</div>
